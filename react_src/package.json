{
  "name": "contact-list-dashboard",
  "version": "0.0.1",
  "description": "Contact List Dashboard React SPA",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && mv build/index.html ../views/index.html && rm -rf ../public/static && mv build/* ../public",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:minline/microservices/dashboard.git"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@popperjs/core": "^2.0.6",
    "admin-lte": "^2.4.8",
    "axios": "^0.21.1",
    "bootstrap": "^3.3.7",
    "history": "^4.7.2",
    "jquery": "^3.3.1",
    "popper": "^1.0.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-js-pagination": "^3.0.3",
    "react-notifications": "^1.4.3",
    "react-paginate": "^6.0.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.3.1",
    "react-router-dom": "^5.1.2",
    "react-router-redux": "^4.0.8",
    "react-scripts": "^3.4.0",
    "react-select": "^3.0.8",
    "redux": "^3.7.2"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "jsx-a11y/anchor-is-valid": [
        "error",
        {
          "components": [
            "Link"
          ],
          "specialLink": [
            "to"
          ]
        }
      ]
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
